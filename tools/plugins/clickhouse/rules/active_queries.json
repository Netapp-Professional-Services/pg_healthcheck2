{
  "check": "active_queries",
  "rules": [
    {
      "condition": "len(structured_data['active_queries']['data']) > 0",
      "severity": "info",
      "message": "Active queries detected."
    },
    {
      "condition": "any(q.get('elapsed', 0) > 10.0 for q in structured_data['active_queries']['data'])",
      "severity": "warning",
      "message": "Slow queries detected (elapsed time > 10 seconds)."
    }
  ]
}
